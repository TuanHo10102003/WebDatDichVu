@model IEnumerable<WebTest.Models.EF.OrderDetail>
@if (Model != null && Model.Any())
{
    int i = 1;
<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Tên dịch vụ</th>
            <th>Giá</th>
            <th>Số lượng</th>
            <th>Thành tiền</th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>@i</td>
    <td>@item.Service.Title</td>
    <td>@item.Price</td>
    <td>@item.Quantity</td>
    <td>@WebTest.Common.Common.FormatNumber((item.Price * item.Quantity), 0)</td>
</tr>
i++;
}
    </tbody>
</table>}
