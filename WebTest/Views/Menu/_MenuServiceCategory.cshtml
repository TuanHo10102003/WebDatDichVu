@model IEnumerable<WebTest.Models.EF.ServiceCategory>
@if (Model != null && Model.Any())
{
<div class="banner">
    <div class="container">
        <div class="row">
            <div class="col text-center">
                <div class="section_title new_arrivals_title">
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
<div class="col-md-4">
    <div class="banner_item align-items-center" style="background-image: url(@item.Icon)">
        <div class="banner_category">
            <a href="/danh-muc-dich-vu/@item.Alias-@item.Id">@item.Title</a>
        </div>
    </div>
</div>  }
        </div>
    </div>
</div>}
